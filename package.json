  },
  "homepage": "https://github.com/MrKhelil/githubStates#readme",
  "dependencies": {
    "axios": "^1.8.4"
    },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "jest": "^29.7.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",  
    "axios": "^1.8.4"

  }
}
